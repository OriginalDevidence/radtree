<?xml version="1.0" encoding="UTF-8"?>
    <web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <description>Proyecto medioambiental de la Universidad de Zaragoza</description>
    <display-name>RadTree</display-name>
    <!-- ================ Servlets ================ -->
    <servlet>
        <servlet-name>Registro</servlet-name>
        <servlet-class>sistinfo.servlets.RegistroServlet</servlet-class>
    </servlet>
    <!-- ============ Servlets Mapping======= -->
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/RegistrarUsuario.do</url-pattern>
    </servlet-mapping>
    <!-- ============ Welcome page ========= -->
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>